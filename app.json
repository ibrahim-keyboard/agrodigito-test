{
  "expo": {
    "name": "agrilink-app",
    "slug": "agrilink-app",
    "version": "1.0.0--preview--2",
    "scheme": "agrilink-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/logo.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Material3",
            "enforceNavigationBarContrast": false
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/splash-icon-light.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mrkeyboard.agrilinkapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "monochromeImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mrkeyboard.agrilinkapp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "90626119-2ca9-4675-88df-3b90b8453fe8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/90626119-2ca9-4675-88df-3b90b8453fe8"
    }
  }
}
